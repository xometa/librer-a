@model RosaMaríaBookstore.Pages.Users.PositionsModel;
@using RosaMaríaBookstore.Models;
@{
    Cargo position = Model.Cargo;
}

<div class="row">
    <div class="col-lg-12">
        <div class=" card text-center mb-0">
            <div class="card-body">
                <h4 class="mb-0 mt-2">@position.Nombre</h4>
                <p class="text-muted font-14">@position.Id</p>
            </div> <!-- end card-body -->
        </div> <!-- end card -->
    </div> <!-- end col-->
</div>